<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/styles.css" />
    <title>All Models in <%= category.name %></title>
  </head>
  <body>
    <%- include('../partials/navbar') %>
    <div class="container">
      <div class="header">
        <h1>Models in <%= category.name %></h1>
        <a href="/items/new">Create New Model</a>
      </div>
      <ul>
        <% items.forEach(item => { %>
        <li>
          <a href="/items/<%= item.id %>"> <%= item.name %> </a>
        </li>
        <% }) %>
      </ul>
    </div>
  </body>
</html>
