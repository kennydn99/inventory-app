<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Items in <%= category.name %></title>
  </head>
  <body>
    <h1>Items in <%= category.name %></h1>
    <ul>
      <% items.forEach(item => { %>
      <li>
        <a href="/items/<%= item.id %>"> <%= item.name %> </a>
      </li>
      <% }) %>
    </ul>
    <a href="/categories">Back to Categories</a>
  </body>
</html>
